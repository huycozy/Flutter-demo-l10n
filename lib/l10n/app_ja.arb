{
  "helloWorld": "「こんにちは世界」",
  "@helloWorld": {
    "description": "hello world"
  },
  "loginFailed": "ログインに失敗しました！\n{message}",
  "@loginFailed": {
    "type": "text",
    "description": "login failed toast",
    "placeholders": {
      "message": {
        "type": "String",
        "example": "Invalid password!"
      }
    }
  },
  "special": "{price} foo {number} zoo",
  "@special": {
    "description": "special toast",
    "placeholders": {
      "number": {
        "type": "int",
        "description": "number of foo"
      },
      "price": {
        "type": "int",
        "description": "price of foo"
      }
    }
  }
}
